{
  "name": "khaos-cli",
  "version": "1.0.0",
  "description": "CLI para gerar arquivos baseados na arquitetura do projeto khaos",
  "main": "dist/index.js",
  "bin": {
    "khaos": "dist/index.js"
  },
  "scripts": {
    "build": "tsc && npm run copy-templates",
    "copy-templates": "cp -r src/templates dist/",
    "dev": "ts-node src/index.ts",
    "dev:atom": "ts-node src/index.ts atom",
    "dev:create": "ts-node src/index.ts create",
    "dev:create:atom": "ts-node src/index.ts create atom",
    "dev:delete": "ts-node src/index.ts delete",
    "dev:delete:atom": "ts-node src/index.ts delete atom",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "test:example": "cd example && yarn test",
    "clean:example": "cd example && yarn clean",
    "lint": "eslint src/**/*.ts",
    "lint:fix": "eslint src/**/*.ts --fix",
    "format": "prettier --write src/**/*.ts",
    "format:check": "prettier --check src/**/*.ts",
    "setup-dev": "npm install && npm run build",
    "clean": "rm -rf dist coverage .khaos-cache",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.54.0",
    "@tanstack/react-query": "^5.81.2",
    "@typescript-eslint/parser": "^8.35.0",
    "ast-types": "^0.14.2",
    "chalk": "^5.4.1",
    "commander": "^12.1.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.0",
    "glob": "^11.0.3",
    "inquirer": "^8.2.6",
    "openai": "^5.7.0",
    "ora": "^8.2.0",
    "zod": "^3.25.67"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/figlet": "^1.7.0",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.11",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "eslint": "^9.29.0",
    "figlet": "^1.8.1",
    "jest": "^29.7.0",
    "prettier": "^3.6.0",
    "ts-jest": "^29.4.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  },
  "keywords": [
    "cli",
    "generator",
    "khaos",
    "architecture",
    "react",
    "react-native",
    "expo",
    "typescript",
    "zod",
    "zod-validation",
    "solid",
    "kiss",
    "composition",
    "clean-code",
    "clean-architecture",
    "clean-code-architecture",
    "clean-code-architecture-react",
    "clean-code-architecture-react-native",
    "clean-code-architecture-expo",
    "expo-router"
  ],
  "author": "Joisiney Leandro",
  "license": "MIT"
}
