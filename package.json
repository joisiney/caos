{
  "name": "khaos-cli",
  "version": "1.0.0",
  "description": "CLI para gerar arquivos baseados na arquitetura do projeto khaos",
  "main": "dist/index.js",
  "bin": {
    "tartarus": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "ts-node src/index.ts",
    "start": "node dist/index.js",
    "prepare": "npm run build",
    "lint": "eslint src/**/*.{ts,tsx}",
    "format": "prettier --write src/**/*.{ts,tsx}"
  },
  "keywords": [
    "cli",
    "generator",
    "khaos",
    "tartarus",
    "architecture",
    "react",
    "react-native",
    "expo",
    "typescript",
    "zod",
    "zod-validation",
    "solid",
    "kiss",
    "composition",
    "clean-code",
    "clean-architecture",
    "clean-code-architecture",
    "clean-code-architecture-react",
    "clean-code-architecture-react-native",
    "clean-code-architecture-expo",
    "expo-router"
  ],
  "author": "Joisiney Leandro",
  "license": "MIT",
  "dependencies": {
    "commander": "^14.0.0",
    "ejs": "^3.1.10",
    "fs-extra": "^11.3.0",
    "inquirer": "^8.2.6",
    "simple-git": "^3.28.0"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^8.2.10",
    "@types/node": "^24.0.1",
    "@typescript-eslint/eslint-plugin": "^8.34.0",
    "@typescript-eslint/parser": "^8.34.0",
    "eslint": "^9.29.0",
    "prettier": "^3.5.3",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.3"
  }
}
